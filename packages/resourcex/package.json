{
  "name": "resourcexjs",
  "version": "1.3.0",
  "description": "ResourceX - AI Resource Management Protocol",
  "keywords": [
    "resourcex",
    "arp",
    "agent",
    "resource",
    "protocol",
    "ai",
    "npm-for-ai"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Deepractice/ResourceX.git"
  },
  "homepage": "https://github.com/Deepractice/ResourceX",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./arp": {
      "types": "./dist/arp.d.ts",
      "default": "./dist/arp.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run build.ts",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@resourcexjs/arp": "workspace:*",
    "@resourcexjs/core": "workspace:*",
    "@resourcexjs/loader": "workspace:*",
    "@resourcexjs/registry": "workspace:*",
    "@resourcexjs/type": "workspace:*"
  },
  "devDependencies": {},
  "publishConfig": {
    "access": "public"
  }
}
